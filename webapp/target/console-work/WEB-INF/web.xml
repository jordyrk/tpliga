<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
<!--  This web.xml is an created automatically by merging
  the web.xml from Aksess Publisering with the one
  from the Aksess project-->
<display-name>@projectname@</display-name>
<description>@projectname@</description>
<!--Context params from the project have precedence-->
<context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext-project.xml</param-value>
    </context-param>
<!--Project filters-->
<filter><filter-name>org.simplericity.jettyconsole.filter.NotFoundFilter</filter-name><filter-class>org.simplericity.jettyconsole.filter.NotFoundFilter</filter-class></filter><filter>
        <filter-name>ParamEncodingFilter</filter-name>
        <filter-class>no.kantega.commons.filter.ParamEncodingFilter</filter-class>
    </filter>
<!--Aksess Publisering filters-->
<filter>
        <filter-name>ResponseHeaderFilter</filter-name>
        <filter-class>no.kantega.commons.filter.ResponseHeaderFilter</filter-class>
        <init-param>
            <param-name>extensions</param-name>
            <param-value>jpg,png,gif</param-value>
        </init-param>
        <init-param>
            <param-name>expiresInDays</param-name>
            <param-value>365</param-value>
        </init-param>
    </filter>
<filter>
        <filter-name>ParamEncodingFilter</filter-name>
        <filter-class>no.kantega.commons.filter.ParamEncodingFilter</filter-class>
    </filter>
<filter>
        <filter-name>MultipartFilter</filter-name>
        <filter-class>no.kantega.commons.filter.NoSpringContextCapableMultipartFilter</filter-class>
    </filter>
<filter>
        <filter-name>AdminFilter</filter-name>
        <filter-class>no.kantega.publishing.security.filter.AdminFilter</filter-class>
    </filter>
<filter>
        <filter-name>ContentRewriteFilter</filter-name>
        <filter-class>no.kantega.publishing.client.filter.ContentRewriteFilter</filter-class>
    </filter>
<filter>
        <filter-name>AksessRequestFilter</filter-name>
        <filter-class>no.kantega.commons.filter.AksessRequestFilter</filter-class>
    </filter>
<filter>
        <filter-name>PluginDelgatingFilter</filter-name>
        <filter-class>no.kantega.publishing.security.PluginDelegatingFilter</filter-class>
    </filter>
<filter>
        <filter-name>aksesswro</filter-name>
        <filter-class>no.kantega.publishing.client.filter.OpenAksessConfiguredFilter</filter-class>
        <init-param>
            <param-name>wrappedFilterClass</param-name>
            <param-value>ro.isdc.wro.http.WroFilter</param-value>
        </init-param>
    </filter>
<filter>
        <filter-name>FakeDirectoryExpires</filter-name>
        <filter-class>no.kantega.commons.filter.FarFutureExpiresDirectoryFilter</filter-class>
    </filter>
<!--AP filter mappings-->
<filter-mapping><filter-name>org.simplericity.jettyconsole.filter.NotFoundFilter</filter-name><url-pattern>/JettyConsoleBootstrapMainClass.class</url-pattern></filter-mapping><filter-mapping>
        <filter-name>FakeDirectoryExpires</filter-name>
        <url-pattern>/expires/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>ResponseHeaderFilter</filter-name>
        <url-pattern>/admin/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>ParamEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>PluginDelgatingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>MultipartFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>AdminFilter</filter-name>
        <url-pattern>/admin/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>ContentRewriteFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>AksessRequestFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<filter-mapping>
        <filter-name>aksesswro</filter-name>
        <url-pattern>/wro-oa/*</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>INCLUDE</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>
<!--Project filter-mappings-->
<filter-mapping>
        <filter-name>ParamEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
<!--Project listeners-->
<!--Aksess publisering listeners-->
<listener>
        <listener-class>no.kantega.publishing.spring.DataDirectoryContextListener</listener-class>
    </listener>
<listener>
        <listener-class>no.kantega.publishing.spring.LogInitListener</listener-class>
    </listener>
<listener>
        <listener-class>no.kantega.publishing.spring.OpenAksessContextLoaderListener</listener-class>
    </listener>
<!--Project servlets-->
<servlet>
        <servlet-name>tpl</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
<!--AP servlets-->
<servlet>
        <servlet-name>aksess</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>
<servlet>
        <servlet-name>useradmin</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>
<servlet>
        <servlet-name>dwr</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>3</load-on-startup>
        <init-param>
            <param-name>debug</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>crossDomainSessionSecurity</param-name>
            <param-value>false</param-value>
        </init-param>
    </servlet>
<servlet>
        <servlet-name>webdav</servlet-name>
        <servlet-class>com.bradmcevoy.http.MiltonServlet</servlet-class>
        <init-param>
            <param-name>resource.factory.factory.class</param-name>
            <param-value>no.kantega.publishing.webdav.AksessResourceFactoryFactory</param-value>
        </init-param>
        <!--
        To disable digest authentication uncomment the following. To completely disable
        all authentication provide an empty value. If this init-param is missing milton
        will default to basic and digest authentication.
        -->
        <init-param>
            <param-name>authentication.handler.classes</param-name>
            <param-value>com.bradmcevoy.http.http11.auth.BasicAuthHandler</param-value>
        </init-param>

        <init-param>
            <param-name>not.found.url</param-name>
            <param-value>/404.jsp</param-value>
        </init-param>
        <init-param>
            <param-name>filter_0</param-name>
            <param-value>com.bradmcevoy.http.DebugFilter</param-value>
        </init-param>
        <load-on-startup>0</load-on-startup>
    </servlet>
<servlet>
        <servlet-name>TinyMCEServlet</servlet-name>
        <servlet-class>no.kantega.publishing.TinyMCEServlet</servlet-class>
    </servlet>
<!--Project servlet-mappings-->
<servlet-mapping>
        <servlet-name>tpl</servlet-name>
        <url-pattern>/tpl/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jjs</url-pattern>
    </servlet-mapping>
<!--Aksess servlet-mappings-->
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/content.ap</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/content/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>*.asp</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/attachment.ap</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/attachments/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/multimedia.ap</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/multimedia/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/rss.ap</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>*.action</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>useradmin</servlet-name>
        <url-pattern>/admin/administration/useradmin/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/admin/oap/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>dwr</servlet-name>
        <url-pattern>/admin/dwr/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/oap/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jnlp</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jjs</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>jsp</servlet-name>
        <url-pattern>*.jsp</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/sitemap.xml</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>aksess</servlet-name>
        <url-pattern>/robots.txt</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>webdav</servlet-name>
        <url-pattern>/webdav/*</url-pattern>
    </servlet-mapping>
<servlet-mapping>
        <servlet-name>TinyMCEServlet</servlet-name>
        <url-pattern>/aksess/tiny_mce/tiny_mce_gzip.jsp</url-pattern>
    </servlet-mapping>
<welcome-file-list>
<!--Project welcome files-->
<!--AP welcome files-->
<welcome-file>content.ap</welcome-file>
<welcome-file>index.jsp</welcome-file>
</welcome-file-list>
<!--Session-timeout from project-->
<session-config>
        <session-timeout>480</session-timeout>
    </session-config>
<!--Project error pages-->
<error-page>
        <error-code>500</error-code>
        <location>/500.jsp</location>
    </error-page>
<!--Aksess error pages-->
<error-page>
        <error-code>404</error-code>
        <location>/content.ap</location>
    </error-page>
</web-app>